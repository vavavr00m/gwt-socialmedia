	$ -->

<module rename-to="showcase">

	<!-- Core configuration -->
	<inherits name="com.tinesoft.gwt.socialmedia.showcase.ShowcaseApp" />
	
	<!-- User Agent (all) -->
	<set-property name="user.agent" value="ie6,ie8,gecko1_8,safari,opera,ie9" />
	
	<!-- i18n -->
	<extend-property name="locale" values="en" />
	<set-property-fallback name="locale" value="en" />

	<!-- GWT Log-->
	<inherits name="com.allen_sauer.gwt.log.gwt-log-WARN" />
	<inherits name="com.allen_sauer.gwt.log.gwt-log-RemoteLogger" />
	<set-property name="log_GWTLogger" value="DISABLED" />
	<set-property name="log_ConsoleLogger" value="DISABLED" />

	<set-property name="log_WindowLogger" value="DISABLED" />
	<set-property name="log_SystemLogger" value="DISABLED" />
	<set-property name="log_FirebugLogger" value="DISABLED" />
	<set-property name="log_DivLogger" value="DISABLED" />
	

</module>
